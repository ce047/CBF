<?xml version="1.0"?>
<ControlBasis>
<Controller xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="PrimitiveControllerType">
 <Name>foo</Name>

 <Coefficient> 1 </Coefficient>

 <ConvergenceCriterion xsi:type="TaskSpaceDistanceThresholdType">
  <Threshold> 0.001 </Threshold>
 </ConvergenceCriterion>

 <Reference xsi:type="XCFVectorReferenceType">
  <Name>controller</Name>
  <Dimension>62</Dimension>
 </Reference>

 <Potential xsi:type="SquarePotentialType">
  <MaxGradientStepNorm> 10 </MaxGradientStepNorm>
  <Dimension> 62 </Dimension>
  <Coefficient> 0.4 </Coefficient>
 </Potential>

 <SensorTransform xsi:type="IdentitySensorTransformType">
  <Dimension> 62 </Dimension>
 </SensorTransform>

 <EffectorTransform xsi:type="GenericEffectorTransformType"/>

 <CombinationStrategy xsi:type="AddingStrategyType"/>

 <Resource xsi:type="CompositeResourceType">
  <Resource xsi:type="RobotInterfaceResourceType">
   <SendMemoryName>xcf:wb</SendMemoryName>
   <RecvMemoryName>xcf:wb</RecvMemoryName>
   <RobotName>LeftArm</RobotName>
   <NumberOfJoints> 7 </NumberOfJoints>
  </Resource>
  <Resource xsi:type="RobotInterfaceResourceType">
   <SendMemoryName>xcf:wb</SendMemoryName>
   <RecvMemoryName>xcf:wb</RecvMemoryName>
   <RobotName>LeftHand</RobotName>
   <NumberOfJoints> 24 </NumberOfJoints>
  </Resource>
  <Resource xsi:type="RobotInterfaceResourceType">
   <SendMemoryName>xcf:wb</SendMemoryName>
   <RecvMemoryName>xcf:wb</RecvMemoryName>
   <RobotName>RightArm</RobotName>
   <NumberOfJoints> 7 </NumberOfJoints>
  </Resource>
  <Resource xsi:type="RobotInterfaceResourceType">
   <SendMemoryName>xcf:wb</SendMemoryName>
   <RecvMemoryName>xcf:wb</RecvMemoryName>
   <RobotName>RightHand</RobotName>
   <NumberOfJoints> 24 </NumberOfJoints>
  </Resource>

 </Resource>

</Controller>
</ControlBasis>
